{
    "id": "dark-sun-calendar",
    "title": "Dark Sun Calendar",
    "description": "Complete Athasian calendar system with King's Ages, lunar cycles, and historical events for the Dark Sun campaign setting",
    "version": "#{VERSION}#",
    "compatibility": {
        "minimum": "13",
        "maximum": "13"
    },
    "authors": [
        {
            "name": "Chris Allbritton",
            "email": "callbritton@mac.com",
            "url": "https://github.com/ctbritt"
        }
    ],
    "esmodules": [
        "scripts/calendar-grid.js",
        "scripts/chat-commands.js",
        "scripts/date-converter.js",
        "scripts/ds-calendar-date.js",
        "scripts/eclipse-engine.js",
        "scripts/main.js",
        "scripts/moon-engine.js"
    ],
    "styles": [
